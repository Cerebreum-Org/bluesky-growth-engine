# Self-hosted/local
SUPABASE_URL=
SUPABASE_SERVICE_ROLE_KEY=

# Cloud/frontend
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# Optional explicit cloud vars for scripts
CLOUD_SUPABASE_URL=
CLOUD_SUPABASE_ANON_KEY=

## Warp baseline env
BLUESKY_HANDLE=
BLUESKY_PASSWORD=
BLUESKY_SERVICE=https://bsky.social
SUPABASE_KEY=
# Optional Supabase aliases
# SUPABASE_SERVICE_ROLE_KEY=
# SUPABASE_ANON_KEY=
API_HOST=0.0.0.0
API_PORT=3001
PROXY_LIST=
